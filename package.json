{
  "name": "universal-website",
  "version": "0.6.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:prod": "ng run universal-website:server:production && npm run compile:server",
    "build:server:serverless": "ng run universal-website:server:serverless && npm run compile:server",
    "compile:server": "webpack --config webpack/webpack.config.ts --progress --colors",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "serve:ssr": "node dist/server",
    "build:ssr": "ng build --prod && ng run universal-website:server:production && npm run compile:server",
    "start:prerender": "npm run build:prerender & npm run serve:prerender",
    "serve:prerender": "node static.js",
    "build:prerender": "npm run build:prod && node dist/prerender.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.8",
    "@angular/common": "~8.2.8",
    "@angular/compiler": "~8.2.8",
    "@angular/core": "~8.2.8",
    "@angular/forms": "~8.2.8",
    "@angular/platform-browser": "~8.2.8",
    "@angular/platform-browser-dynamic": "~8.2.8",
    "@angular/platform-server": "~8.2.8",
    "@angular/pwa": "^0.803.6",
    "@angular/router": "~8.2.8",
    "@angular/service-worker": "~8.2.8",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@ng-toolkit/serverless": "^7.1.2",
    "@ng-toolkit/universal": "^7.1.2",
    "@nguniversal/common": "8.1.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "aws-serverless-express": "^3.3.6",
    "bootstrap": "^4.3.1",
    "cors": "~2.8.5",
    "cp-cli": "^2.0.0",
    "domino": "^2.1.3",
    "express": "^4.17.1",
    "ngx-bootstrap": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "~8.3.6",
    "@angular/compiler-cli": "~8.2.8",
    "@angular/language-service": "~8.2.8",
    "@types/express": "^4.17.1",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.7.7",
    "@types/webpack": "^4.39.1",
    "codelyzer": "^5.1.1",
    "husky": "^3.0.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "opencollective": "^1.0.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.2",
    "serverless": "1.53.0",
    "serverless-api-compression": "^1.0.1",
    "serverless-apigw-binary": "^0.4.4",
    "ts-loader": "6.1.2",
    "ts-node": "^8.4.1",
    "tslint": "~5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint"
    }
  }
}
